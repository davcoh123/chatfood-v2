---
import DashboardLayout from '@/layouts/DashboardLayout.astro';
import AnalyticsPage from '@/components/react/AnalyticsPage';

// Get user from middleware locals
const user = (Astro.locals as any).user;

if (!user) {
  return Astro.redirect('/login?redirect=/dashboard/analytics');
}
---

<DashboardLayout title="Analytics">
  <AnalyticsPage client:only="react" />
</DashboardLayout>
