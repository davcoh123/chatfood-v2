---
import DashboardLayout from '@/layouts/DashboardLayout.astro';
import CustomersAnalyticsPage from '@/components/react/analytics/CustomersAnalyticsPage';

// Get user from middleware locals
const user = (Astro.locals as any).user;

if (!user) {
  return Astro.redirect('/login?redirect=/dashboard/analytics/customers');
}
---

<DashboardLayout title="Analytics - Clients">
  <CustomersAnalyticsPage client:only="react" />
</DashboardLayout>
